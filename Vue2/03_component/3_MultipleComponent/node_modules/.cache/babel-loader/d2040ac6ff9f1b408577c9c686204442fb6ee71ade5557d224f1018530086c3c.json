entName(name){\n        console.log('App get Student name:',name)\n      }\n    },\n    mounted(){\n      this.$refs.student.$on('chenyunfeng',this.getStudentName)\n    }\n  }\n</script>\n\n<!--scoped一般不用于App，因为App中的style一般用于全局-->\n<style scoped>\n  .app{\n    background-color: gray;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}