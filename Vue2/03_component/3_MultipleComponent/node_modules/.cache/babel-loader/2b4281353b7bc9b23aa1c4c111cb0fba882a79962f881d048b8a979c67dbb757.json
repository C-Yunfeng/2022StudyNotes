{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport MyHeader from \"@/components/MyHeader\";\nimport MyList from \"@/components/MyList\";\nimport MyFooter from \"@/components/MyFooter\";\nexport default {\n  name: \"App\",\n  components: {\n    MyFooter,\n    MyList,\n    MyHeader\n  },\n  data() {\n    return {\n      // 方法1：watch实现\n      todos: JSON.parse(localStorage.getItem('todosKey')) || []\n    };\n  },\n  methods: {\n    createItem(item) {\n      this.todos.unshift(item);\n    },\n    checkItem(id) {\n      this.todos.forEach(x => {\n        if (x.id === id) x.done = !x.done;\n      });\n    },\n    deleteItem(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    checkAllItem(done) {\n      this.todos.forEach(x => {\n        x.done = done;\n      });\n    },\n    clearAllItem() {\n      this.todos = this.todos.filter(todo => {\n        return !todo.done;\n      });\n    }\n  },\n  // 方法1：watch实现\n  watch: {\n    todos: {\n      deep: true,\n      handler(newV) {\n        localStorage.setItem('todosKey', JSON.stringify(newV));\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAqBE,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,MAAK,MAAO,qBAAqB;AACxC,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAC;IAACF,QAAQ;IAACD,MAAM;IAACD;EAAQ,CAAC;EACrCK,IAAI,GAAE;IACJ,OAAO;MACL;MACAC,KAAK,EAACC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,KAAK;IACxD;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,UAAU,CAACC,IAAI,EAAC;MACd,IAAI,CAACP,KAAK,CAACQ,OAAO,CAACD,IAAI;IACzB,CAAC;IACDE,SAAS,CAACC,EAAE,EAAC;MACX,IAAI,CAACV,KAAK,CAACW,OAAO,CAAEC,CAAC,IAAG;QACtB,IAAGA,CAAC,CAACF,EAAE,KAAGA,EAAE,EAAEE,CAAC,CAACC,IAAI,GAAC,CAACD,CAAC,CAACC,IAAG;MAC7B,CAAC;IACH,CAAC;IACDC,UAAU,CAACJ,EAAE,EAAC;MACZ,IAAI,CAACV,KAAK,GAAC,IAAI,CAACA,KAAK,CAACe,MAAM,CAACC,IAAI,IAAEA,IAAI,CAACN,EAAE,KAAGA,EAAE;IACjD,CAAC;IACDO,YAAY,CAACJ,IAAI,EAAC;MAChB,IAAI,CAACb,KAAK,CAACW,OAAO,CAAEC,CAAC,IAAG;QACtBA,CAAC,CAACC,IAAG,GAAIA,IAAG;MACd,CAAC;IACH,CAAC;IACDK,YAAY,GAAE;MACZ,IAAI,CAAClB,KAAI,GAAI,IAAI,CAACA,KAAK,CAACe,MAAM,CAAEC,IAAI,IAAG;QACrC,OAAO,CAACA,IAAI,CAACH,IAAG;MAClB,CAAC;IACH;EACF,CAAC;EACD;EACAM,KAAK,EAAC;IACJnB,KAAK,EAAC;MACJoB,IAAI,EAAC,IAAI;MACTC,OAAO,CAACC,IAAI,EAAC;QACXnB,YAAY,CAACoB,OAAO,CAAC,UAAU,EAACtB,IAAI,CAACuB,SAAS,CAACF,IAAI,CAAC;MACtD;IACF;EACF;AACF","names":["MyHeader","MyList","MyFooter","name","components","data","todos","JSON","parse","localStorage","getItem","methods","createItem","item","unshift","checkItem","id","forEach","x","done","deleteItem","filter","todo","checkAllItem","clearAllItem","watch","deep","handler","newV","setItem","stringify"],"sourceRoot":"","sources":["/Users/chenyunfeng/Github_Pj/2022StudyNotes/Vue2/03_component/3_MultipleComponent/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"root\">\n    <div class=\"todo-container\">\n      <div class=\"todo-wrap\">\n        <MyHeader :itemCreated=\"createItem\"></MyHeader>\n        <MyList\n            :todos=\"todos\"\n            :checkItem=\"checkItem\"\n            :deleteItem=\"deleteItem\">\n        </MyList>\n        <MyFooter\n            :todos=\"todos\"\n            :checkAllItem=\"checkAllItem\"\n            :clearAllItem=\"clearAllItem\">\n        </MyFooter>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import MyHeader from \"@/components/MyHeader\";\n  import MyList from \"@/components/MyList\";\n  import MyFooter from \"@/components/MyFooter\";\n\n  export default {\n    name: \"App\",\n    components:{MyFooter,MyList,MyHeader},\n    data(){\n      return {\n        // 方法1：watch实现\n        todos:JSON.parse(localStorage.getItem('todosKey')) || []\n      }\n    },\n    methods:{\n      createItem(item){\n        this.todos.unshift(item)\n      },\n      checkItem(id){\n        this.todos.forEach((x)=>{\n          if(x.id===id) x.done=!x.done\n        })\n      },\n      deleteItem(id){\n        this.todos=this.todos.filter(todo=>todo.id!==id)\n      },\n      checkAllItem(done){\n        this.todos.forEach((x)=>{\n          x.done = done\n        })\n      },\n      clearAllItem(){\n        this.todos = this.todos.filter((todo)=>{\n          return !todo.done\n        })\n      }\n    },\n    // 方法1：watch实现\n    watch:{\n      todos:{\n        deep:true,\n        handler(newV){\n          localStorage.setItem('todosKey',JSON.stringify(newV))\n        }\n      }\n    }\n  }\n</script>\n\n<!--scoped一般不用于App，因为App中的style一般用于全局-->\n<style>\n  /*base*/\n  body {\n    background: #fff;\n  }\n\n  .btn {\n    display: inline-block;\n    padding: 4px 12px;\n    margin-bottom: 0;\n    font-size: 14px;\n    line-height: 20px;\n    text-align: center;\n    vertical-align: middle;\n    cursor: pointer;\n    box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);\n    border-radius: 4px;\n  }\n\n  .btn-danger {\n    color: #fff;\n    background-color: #da4f49;\n    border: 1px solid #bd362f;\n  }\n\n  .btn-danger:hover {\n    color: #fff;\n    background-color: #bd362f;\n  }\n\n  .btn:focus {\n    outline: none;\n  }\n\n  .todo-container {\n    width: 600px;\n    margin: 0 auto;\n  }\n  .todo-container .todo-wrap {\n    padding: 10px;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}