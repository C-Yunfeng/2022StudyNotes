{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport MyHeader from \"@/components/MyHeader\";\nimport MyList from \"@/components/MyList\";\nimport MyFooter from \"@/components/MyFooter\";\nexport default {\n  name: \"App\",\n  components: {\n    MyFooter,\n    MyList,\n    MyHeader\n  },\n  data() {\n    return {\n      todos: [{\n        id: '001',\n        title: '吃饭',\n        done: true\n      }, {\n        id: '002',\n        title: '睡觉',\n        done: false\n      }, {\n        id: '003',\n        title: '打豆豆',\n        done: true\n      }]\n    };\n  },\n  methods: {\n    createItem(item) {\n      this.todos.unshift(item);\n    },\n    checkItem(id) {\n      this.todos.forEach(x => {\n        if (x.id === id) x.done = !x.done;\n      });\n    },\n    deleteItem(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    checkAllItem(done) {\n      this.todos.forEach(x => {\n        this.done = done;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAiBE,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,MAAK,MAAO,qBAAqB;AACxC,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAC;IAACF,QAAQ;IAACD,MAAM;IAACD;EAAQ,CAAC;EACrCK,IAAI,GAAE;IACJ,OAAO;MACLC,KAAK,EAAC,CACJ;QAACC,EAAE,EAAC,KAAK;QAACC,KAAK,EAAC,IAAI;QAACC,IAAI,EAAC;MAAI,CAAC,EAC/B;QAACF,EAAE,EAAC,KAAK;QAACC,KAAK,EAAC,IAAI;QAACC,IAAI,EAAC;MAAK,CAAC,EAChC;QAACF,EAAE,EAAC,KAAK;QAACC,KAAK,EAAC,KAAK;QAACC,IAAI,EAAC;MAAI,CAAC;IAEpC;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,UAAU,CAACC,IAAI,EAAC;MACd,IAAI,CAACN,KAAK,CAACO,OAAO,CAACD,IAAI;IACzB,CAAC;IACDE,SAAS,CAACP,EAAE,EAAC;MACX,IAAI,CAACD,KAAK,CAACS,OAAO,CAAEC,CAAC,IAAG;QACtB,IAAGA,CAAC,CAACT,EAAE,KAAGA,EAAE,EAAES,CAAC,CAACP,IAAI,GAAC,CAACO,CAAC,CAACP,IAAG;MAC7B,CAAC;IACH,CAAC;IACDQ,UAAU,CAACV,EAAE,EAAC;MACZ,IAAI,CAACD,KAAK,GAAC,IAAI,CAACA,KAAK,CAACY,MAAM,CAACC,IAAI,IAAEA,IAAI,CAACZ,EAAE,KAAGA,EAAE;IACjD,CAAC;IACDa,YAAY,CAACX,IAAI,EAAC;MAChB,IAAI,CAACH,KAAK,CAACS,OAAO,CAAEC,CAAC,IAAG;QACtB,IAAI,CAACP,IAAG,GAAIA,IAAG;MACjB,CAAC;IACH;EACF;AACF","names":["MyHeader","MyList","MyFooter","name","components","data","todos","id","title","done","methods","createItem","item","unshift","checkItem","forEach","x","deleteItem","filter","todo","checkAllItem"],"sourceRoot":"","sources":["/Users/chenyunfeng/Github_Pj/2022StudyNotes/Vue2/component/3_SingleComponent/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"root\">\n    <div class=\"todo-container\">\n      <div class=\"todo-wrap\">\n        <MyHeader :itemCreated=\"createItem\"></MyHeader>\n        <MyList\n            :todos=\"todos\"\n            :checkItem=\"checkItem\"\n            :deleteItem=\"deleteItem\">\n        </MyList>\n        <MyFooter :todos=\"todos\" :checkAllItem=\"checkAllItem\"></MyFooter>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import MyHeader from \"@/components/MyHeader\";\n  import MyList from \"@/components/MyList\";\n  import MyFooter from \"@/components/MyFooter\";\n\n  export default {\n    name: \"App\",\n    components:{MyFooter,MyList,MyHeader},\n    data(){\n      return {\n        todos:[\n          {id:'001',title:'吃饭',done:true},\n          {id:'002',title:'睡觉',done:false},\n          {id:'003',title:'打豆豆',done:true},\n        ]\n      }\n    },\n    methods:{\n      createItem(item){\n        this.todos.unshift(item)\n      },\n      checkItem(id){\n        this.todos.forEach((x)=>{\n          if(x.id===id) x.done=!x.done\n        })\n      },\n      deleteItem(id){\n        this.todos=this.todos.filter(todo=>todo.id!==id)\n      },\n      checkAllItem(done){\n        this.todos.forEach((x)=>{\n          this.done = done\n        })\n      }\n    }\n  }\n</script>\n\n<!--scoped一般不用于App，因为App中的style一般用于全局-->\n<style>\n  /*base*/\n  body {\n    background: #fff;\n  }\n\n  .btn {\n    display: inline-block;\n    padding: 4px 12px;\n    margin-bottom: 0;\n    font-size: 14px;\n    line-height: 20px;\n    text-align: center;\n    vertical-align: middle;\n    cursor: pointer;\n    box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);\n    border-radius: 4px;\n  }\n\n  .btn-danger {\n    color: #fff;\n    background-color: #da4f49;\n    border: 1px solid #bd362f;\n  }\n\n  .btn-danger:hover {\n    color: #fff;\n    background-color: #bd362f;\n  }\n\n  .btn:focus {\n    outline: none;\n  }\n\n  .todo-container {\n    width: 600px;\n    margin: 0 auto;\n  }\n  .todo-container .todo-wrap {\n    padding: 10px;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}