{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport MyHeader from \"@/components/MyHeader\";\nimport MyList from \"@/components/MyList\";\nimport MyFooter from \"@/components/MyFooter\";\nexport default {\n  name: \"App\",\n  components: {\n    MyFooter,\n    MyList,\n    MyHeader\n  },\n  data() {\n    return {\n      // 方法1：watch实现\n      // todos:JSON.parse(localStorage.getItem('todosKey')) || []\n    };\n  },\n  methods: {\n    createItem(item) {\n      this.todos.unshift(item);\n    },\n    checkItem(id) {\n      this.todos.forEach(x => {\n        if (x.id === id) x.done = !x.done;\n      });\n    },\n    deleteItem(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    checkAllItem(done) {\n      this.todos.forEach(x => {\n        x.done = done;\n      });\n    },\n    clearAllItem() {\n      this.todos = this.todos.filter(todo => {\n        return !todo.done;\n      });\n    }\n  },\n  updated() {}\n  // 方法1：watch实现\n  // watch:{\n  //   todos:{\n  //     deep:true,\n  //     handler(newV){\n  //       localStorage.setItem('todosKey',JSON.stringify(newV))\n  //     }\n  //   }\n  // }\n};","map":{"version":3,"mappings":";AAqBE,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,MAAK,MAAO,qBAAqB;AACxC,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAC;IAACF,QAAQ;IAACD,MAAM;IAACD;EAAQ,CAAC;EACrCK,IAAI,GAAE;IACJ,OAAO;MACL;MACA;IAAA,CACF;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,UAAU,CAACC,IAAI,EAAC;MACd,IAAI,CAACC,KAAK,CAACC,OAAO,CAACF,IAAI;IACzB,CAAC;IACDG,SAAS,CAACC,EAAE,EAAC;MACX,IAAI,CAACH,KAAK,CAACI,OAAO,CAAEC,CAAC,IAAG;QACtB,IAAGA,CAAC,CAACF,EAAE,KAAGA,EAAE,EAAEE,CAAC,CAACC,IAAI,GAAC,CAACD,CAAC,CAACC,IAAG;MAC7B,CAAC;IACH,CAAC;IACDC,UAAU,CAACJ,EAAE,EAAC;MACZ,IAAI,CAACH,KAAK,GAAC,IAAI,CAACA,KAAK,CAACQ,MAAM,CAACC,IAAI,IAAEA,IAAI,CAACN,EAAE,KAAGA,EAAE;IACjD,CAAC;IACDO,YAAY,CAACJ,IAAI,EAAC;MAChB,IAAI,CAACN,KAAK,CAACI,OAAO,CAAEC,CAAC,IAAG;QACtBA,CAAC,CAACC,IAAG,GAAIA,IAAG;MACd,CAAC;IACH,CAAC;IACDK,YAAY,GAAE;MACZ,IAAI,C